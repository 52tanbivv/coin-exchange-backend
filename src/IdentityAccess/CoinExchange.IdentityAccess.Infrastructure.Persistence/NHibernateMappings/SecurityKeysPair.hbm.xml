<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
assembly="CoinExchange.IdentityAccess.Domain.Model" namespace="CoinExchange.IdentityAccess.Domain.Model.SecurityKeysAggregate">
  <class name="SecurityKeysPair" table="SecurityKeysPair" lazy="false">
    <id name="KeyDescription" column="KeyDescription">
      <generator class="assigned" />
    </id>
    <property name="UserName"  column="UserName"/>
    <property name="ExpirationDate"  column="ExpirationDate"/>
    <property name="StartDate"  column="StartDate"/>
    <property name="EndDate"  column="EndDate"/>
    <property name="LastModified"  column="LastModified"/>
    <property name="SystemGenerated" column="SystemGenerated"/>
    <component name="ApiKey">
      <property name="Value" column="ApiKey"/>
    </component>
    <component name="SecretKey">
      <property name="Value" column="SecretKey"/>
    </component>
    <bag name="_securityKeysPermissions" table="SecurityKeysPermission" cascade="all" inverse="true" lazy="false">
      <key column="KeyDescription"/>
      <one-to-many
         class="SecurityKeysPermission"/>
    </bag>
  </class>
</hibernate-mapping>